#11720

#include <stdio.h>

int main(void){
    int n;
    scanf("%d ",&n);
    
    int sum=0;
    while(n--){
         char ch;
        scanf("%c", &ch);
        sum += ch - '0';
}
printf("%d\n", sum);
}

#11721

#include <stdio.h>

int main(void){
    char a[100];
    scanf("%s", a);
    
    int b = -1;
    while (a[++b]){
        printf("%c", b[a]);
        if (b%10==9) puts("");
    }
    
}

#2741

#include <stdio.h>

int main(void){
    int a=0;
    scanf("%d", &a);
    for(int i=1;i < a+1;i++){
        printf("%d \n", i);
}
    
}
