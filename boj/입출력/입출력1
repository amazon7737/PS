#10950
#include <stdio.h>

int main(void){
    int a;
    int b;
    int c;
    scanf("%d", &c);
    int i;
    for (i=0;i<c;i++){
        scanf("%d %d",&a, &b );
        printf("%d\n", a+b);
    }
}

#10951
#include <stdio.h>

int main(void){
    int a;
    int b;
    while(scanf("%d %d", &a, &b)!=EOF)
    {
     printf("%d\n", a+b);
    }
    
}

#10952
#include <stdio.h>

int main(void){
    int a;
    int b;
    while(1){
        scanf("%d %d", &a, &b);
    if (a==0 && b==0){
        break;    
    }
        printf("%d\n", a+b);
    }
    
}
#10953
#include <stdio.h>

int main(void){
    int a;
    int b;
    int c;
    scanf("%d", &c);
    for(int i = 0 ; i < c ; i++ ){
        scanf("%d, %d", &a, &b);
        printf("%d\n", a+b);
    }
        
}
#11021
#include <stdio.h>

int main(void){
    int a, b;
    int c;
    scanf("%d", &c);
    for(int i=0;i<c;i++){
        scanf("%d %d", &a, &b);
        printf("Case #%d: %d\n", i+1 ,a+b);
    }
}

#11022

#include <stdio.h>

int main(void){
    int n;
    int a;
    int b;
    scanf("%d", &n);
    for(int i=0;i<n;i++){
        scanf("%d %d", &a, &b);
        printf("Case #%d: %d + %d = %d\n", i+1, a, b, a+b);
        
}

}

